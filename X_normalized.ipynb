   "source": [
    "import numpy as np\n",
    "\n",
    "X = np.random.random((5,5))\n",
    "STD = X.std()\n",
    "M = X.mean()\n",
    "W = (X - M)\n",
    "Z = W/STD\n",
    "\n",
    "np.save(\"X_normalized.npy\", Z)\n",
    "\n",
    "print(\"Normal Array: \\n\", X)\n",
    "print(\"Normalized Array: \\n\", Z)"
   ]
